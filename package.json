  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "upload:dev": "localazy upload -c localazy-dev.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node main.js",
    "loadtest": "ts-node-dev -r tsconfig-paths/register loadtest/server/loadtest.ts loadtest/client/loadtest.ts --numClients $npm_config_clients --endpoint $npm_config_url --room main_room --delay 400",
